		<!--线程数量，测试下来还是1最快，所以就不要改了-->
		<threadsnum>1</threadsnum>
	</合并CSV-txt模式>
	<合并CSV-里程模式>
		<!--原数据路径-->
		<orignaldatapath>D:\TEST\input</orignaldatapath>
		<!--保存路径-->
		<savedatapath>D:\TEST\Combined</savedatapath>
		<!--选择的日期文件，可以多写，以逗号分割，也可以写all，就是选择所有的日期文件-->
		<selectdatefile>all</selectdatefile>
		<!--速度的列号-->
		<spdindex>3</spdindex>
		<!--单位里程值单位是米-->
		<mileagebasis>10000</mileagebasis>
		<!--线程数量，测试下来还是1最快，所以就不要改了-->
		<threadsnum>1</threadsnum>
	</合并CSV-里程模式>
	<去除CSV头部信息>
		<!--原数据路径-->
		<orignaldatapath>D:\TEST\input</orignaldatapath>
		<!--保存路径-->
		<savedatapath>D:\TEST\Combined</savedatapath>
	</去除CSV头部信息>
</configuration>